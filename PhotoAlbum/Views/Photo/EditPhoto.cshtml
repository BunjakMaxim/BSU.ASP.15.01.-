@model IEnumerable<string>

@{
    ViewBag.Title = "Добавленеие тегов";
}
<script src="@Url.Content("~/scripts/jquery-2.2.0.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
<div>
    @using (Ajax.BeginForm("TagAdd","Photo", new { idPhoto = @ViewBag.Id }, new AjaxOptions { UpdateTargetId = "results" }))
    {
        <input type="text" name="nameTag" />
        <input type="submit" value="Поиск" />
    }
    <div id="results">
            <h3>Все тиги фотографии:</h3>
            <ul>
                @foreach (var item in Model)
                {
                    <li>@item</li>
                }
            </ul>
    </div>
</div>
